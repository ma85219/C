<head>
	</head>
	<body>
	<p id="aa"></p>
	<p id="bb"></p>
	<script>
			
		class Point {
		constructor(x, y) {
			this.x = x;
			this.y = y;
		}

		static distance(a, b) {
			const dx = a.x - b.x;
			const dy = a.y - b.y;

			return Math.hypot(dx, dy);
				}
			}

		const p1 = new Point(5, 5);
		const p2 = new Point(10, 10);

		document.write(Point.distance(p1, p2)); 
			
		
		</script>
	</body>
	</html>
